#!/bin/bash

cd /tmp/ci/sync/

git clone https://github.com/Redmi-MT6768/android_device_xiaomi_lava -b eleven device/xiaomi/lava
git clone https://github.com/Redmi-MT6768/android_vendor_xiaomi_lava -b eleven vendor/xiaomi/lava 
